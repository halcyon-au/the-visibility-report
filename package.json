    "axios": "^0.26.1",
    "axios-retry": "^3.2.4",
    "framer-motion": "6.3.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-query": "4.0.0-beta.5",
    "react-router-dom": "^6.3.0",
    "sass": "^1.50.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.2",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vitejs/plugin-react": "^1.3.1",
    "autoprefixer": "^10.4.4",
    "eslint": "^8.13.0",
